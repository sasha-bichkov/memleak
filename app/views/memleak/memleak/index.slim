header
  h1 Dashboard

#chart.chart

table
  thead
    tr
      th id
      th Controller#action
      th Total allocated objects
      th Total allocated memsize
      th Total retained objects
      th Total retained memsize
      th Created At

  tbody
    - @memory_snapshots.each do |snapshot|
      tr
        td = link_to "##{snapshot.id}", snapshot_path(snapshot)
        td = snapshot_request snapshot
        td = snapshot.total_allocated_objects
        td = snapshot.total_allocated_memsize
        td = snapshot.total_retained_objects
        td = snapshot.total_retained_memsize
        td = format_time snapshot.created_at
